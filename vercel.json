{
  "version": 2,
  "builds": [
    {
      "src": "bull-bear-dashboard-main/package.json",
      "use": "@vercel/static-build",
      "config": { 
        "distDir": "dist",
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}